        <Frame type="Control" name="Dropzone">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/MAIN_Background" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/MAIN_Background_stash" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

            <Frame type="Image" name="BG">
                <Anchor relative="$parent"/>
                <Color val="255,125,0"/>
                <Alpha val="0"/>
            </Frame>

            <RenderPriority val="9999"/>
            <AcceptsMouse val="true"/>
            <Animation name="ShowHide">
                <Event event="ECL_ShowSlot" action="DirectionForward, Play" frame="$parent"/>
                <Event event="ECL_HideSlot" action="DirectionReverse, Play" frame="$parent"/>
                <Controller type="Property" end="Pause" frame="$this" property="AcceptsMouse">
                    <Key type="Property" time="0.0" value="false"/>
                    <Key type="Property" time="0.0" value="true"/>
                </Controller>
            </Animation>
        </Frame>

