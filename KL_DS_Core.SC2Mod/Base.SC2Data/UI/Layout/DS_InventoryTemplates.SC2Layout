        <Animation name="ShowHideStash">
            <Event event="ShowStash" action="DirectionForward,Play" frame="$this"/>
            <Event event="HideStash" action="DirectionReverse,Play" frame="$this"/>
            <Controller type="Anchor" end="Pause" frame="$this/MAIN_Background_stash" side="Left" relative="True">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.0625" value="740" inout="Slow"/>
            </Controller>
        </Animation>

        <Frame type="Image" name="MAIN_Background_stash">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="-740"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Width val="#Inventory_Width"/>
            <Texture val="Assets/Textures/ECL_BORDER_INV.dds"/>
            <TextureType val="NineSlice"/>
        </Frame>
    
        <Frame type="Frame" name="AnchorFrame_stash">
            <Anchor side="Top" relative="$parent/MAIN_Background_stash" pos="Min" offset="15"/>
            <Anchor side="Bottom" relative="$parent/MAIN_Background_stash" pos="Max" offset="-15"/>
            <Anchor side="Left" relative="$parent/MAIN_Background_stash" pos="Min" offset="15"/>
            <Anchor side="Right" relative="$parent/MAIN_Background_stash" pos="Max" offset="-15"/>
        </Frame>

